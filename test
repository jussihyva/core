# include "inc/core.h"

t_size	iters = 100000000;

static inline __attribute__((always_inline))
void	swapu8(t_uint8 *a, t_uint8 *b)
{
	*a ^= *b;
	*b ^= *a;
	*a ^= *b;
}

int main(void)
{
	char	*a = s_dup("00001111000011110000111100001111000011110000111100001111000011110000111100001111000011110000111100001111000011110000111100001111000011110000111100001111000011110000111100001111");
	char	*b = s_dup("11001100110011001100110011001100110011001100110011001100110011001100110011001100110011001100110011001100110011001100110011001100110011001100110011001100110011001100110011001100");
	t_size	i;

	i = 0;
	while (i < iters)
	{
		mswap(a, b, s_len(a));
		i++;
	}
	print("%s\n", a);
	print("%s\n", b);
}
